#include<stdio.h>

struct stud{
                float reg;int s1,s2,s3,s4,s5;char name[50];
            }o[2];
void print(int i)
        {
                int count=0;
                printf("\n\nEntered RegistrationNo. Of Student: %f",o[i].reg);
                printf("\nEntered Name Of The Student %s: ",o[i].name);
                printf("\nEntered Marks Of Subject %d: %d",count+1,o[i].s1);
                printf("\nEntered Marks Of Subject %d: %d",count+2,o[i].s2);
                printf("\nEntered Marks Of Subject %d: %d",count+3,o[i].s3);
                printf("\nEntered Marks Of Subject %d: %d",count+4,o[i].s4);
                printf("\nEntered Marks Of Subject %d: %d",count+5,o[i].s5);
        }
void main()

        {
            for(int i=1;i<=2;i++)
            {
                int count=0;
                printf("Enter Registration No.Of Student %d:",i);
                scanf("%f",&o[i].reg);
                printf("Enter Name Of The Student %d:",i);
                scanf("%s",&o[i].name);
                printf("Enter Marks Of Subject %d:",count+1);
                scanf("%d",&o[i].s1);
                printf("Enter Marks Of Subject %d:",count+2);
                scanf("%d",&o[i].s2);
                printf("Enter Marks Of Subject %d:",count+3);
                scanf("%d",&o[i].s3);
                printf("Enter Marks Of Subject %d:",count+4);
                scanf("%d",&o[i].s4);
                printf("Enter Marks Of Subject %d:",count+5);
                scanf("%d",&o[i].s5);
            }
            for(int i=1;i<=2;i++)
            {
                print(i);
            }

        }
